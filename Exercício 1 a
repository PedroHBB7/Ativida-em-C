#include <stdio.h>

void somaNumeros() {
    int array[10];
    int soma = 0;

    printf("Digite 10 números:\n");
    for (int i = 0; i < 10; i++) {
        printf("Número %d: ", i + 1);
        scanf("%d", &array[i]);
        soma += array[i];
    }

    printf("A soma é igual a: %d\n", soma);
}

int main() {
    somaNumeros();
    return 0;
}
